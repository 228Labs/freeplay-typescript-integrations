{
  "name": "@freeplayai/vercel",
  "version": "0.1.2",
  "description": "OpenTelemetry instrumentation for Vercel AI SDK with Freeplay integration - production-ready tracing and telemetry.",
  "keywords": [
    "freeplay",
    "sdk",
    "typescript",
    "vercel",
    "ai",
    "opentelemetry",
    "otel",
    "telemetry",
    "tracing",
    "observability",
    "llm",
    "vercel-ai-sdk"
  ],
  "license": "MIT",
  "author": "228Labs",
  "repository": {
    "type": "git",
    "url": "https://github.com/228labs/freeplay-typescript-integrations.git",
    "directory": "packages/vercel"
  },
  "homepage": "https://github.com/freeplay-ai/vercel#readme",
  "bugs": {
    "url": "https://github.com/freeplay-ai/vercel/issues"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/google": "^2.0.0",
    "@ai-sdk/google-vertex": "^3.0.0",
    "@ai-sdk/openai": "^2.0.0",
    "@arizeai/openinference-vercel": "^2.3.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@vercel/otel": "^2.0.1",
    "ai": "^5.0.0",
    "freeplay": "^0.5.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/google-vertex": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.37",
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/google-vertex": "^3.0.53",
    "@ai-sdk/openai": "^2.0.53",
    "@arizeai/openinference-vercel": "^2.3.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@types/node": "^24.9.1",
    "@vercel/otel": "^2.0.1",
    "ai": "^5.0.77",
    "freeplay": "^0.5.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
